		      <div class="card-body">
		        <p>Programming Application Platform</p>
				<canvas  id="pap-radar"></canvas>
		      </div>
		    </div>
		  </div>
		</div>	

		<div class="row">
		  <div class="col-sm-6">
		    <div class="card">
		      <div class="card-body">
		        <p>IP</p>
				<canvas  id="ips-radar"></canvas>
		      </div>
		    </div>
		  </div>
		  <div class="col-sm-6">
		    <div class="card">
		      <div class="card-body">
		        <p>Innovation</p>
				<canvas  id="ino-radar"></canvas>
		      </div>
		    </div>
		  </div>
		</div>	

